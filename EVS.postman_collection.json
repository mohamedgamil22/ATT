		{
			"name": "Querying the Job Status",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "53b39f71-e9fe-482b-a0b9-8c244b57f33f",
						"type": "text/javascript",
						"exec": [
							"pm.globals.unset(\"variable_key\");",
							"",
							"var jsonData = JSON.parse(responseBody);",
							"",
							"",
							"pm.globals.set(\"status\", jsonData.status);",
							"if ( pm.variables.get(\"status\") == \"SUCCESS\") {",
							"    console.log(\"status: SUCCESS \");",
							"    pm.globals.set(\"volume_id\", jsonData.entities.volume_id);",
							"    postman.setNextRequest(\"Creating an EVS Disk (Native OpenStack API)\");",
							"} else if (pm.variables.get(\"status\") == \"FAIL\") {",
							"    console.log(\"status: FAIL \",jsonData);",
							"    postman.setNextRequest(null);",
							"    return;",
							"} else {",
							"    console.log(\"status: \", pm.variables.get(\"status\"));",
							"    setTimeout(function(){}, 60000); ",
							"    postman.setNextRequest(\"Querying the Job Status\");",
							"} ",
							""
						]
					}
				}
			],
